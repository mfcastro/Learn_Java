import java.util.Scanner;

public class CreateRobot {
	
	public String SelectRobotType(){
		Scanner scan = new Scanner(System.in);
		System.out.println("Standard Robot [r] , Humanoid [h] , Military [m]");
		String selection = scan.next();
		return selection;
	}
